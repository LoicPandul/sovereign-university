{
  "i18n-ally.autoDetection": true,
  "i18n-ally.editor.preferEditor": true,
  "i18n-ally.enabledFrameworks": ["react"],
  "i18n-ally.enabledParsers": ["json"],
  "i18n-ally.extract.autoDetect": true,
  "i18n-ally.keysInUse": ["publicCommunication.blogCategories.all"],
  "i18n-ally.keystyle": "nested",
  "i18n-ally.localesPaths": ["apps/web/public/locales"],
  "i18n-ally.sortKeys": true,
  "i18n-ally.sourceLanguage": "en",
  "search.exclude": {
    "**/tsconfig.*.tsbuildinfo": true
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "tailwindCSS.classAttributes": [
    "class",
    "className",
    "ngClass",
    "class:list",
    ".*Style*",
    ".*Styles*",
    ".*Classes*",
    ".*Class*"
  ],
  "tailwindCSS.experimental.classRegex": [
    ["cva\\(([^)]*)\\)", "[\"'`]([^\"'`]*).*?[\"'`]"],
    ["cx\\(([^)]*)\\)", "(?:'|\"|`)([^']*)(?:'|\"|`)"]
  ],
  "cSpell.words": [
    "Ajelex",
    "Asio",
    "basepath",
    "bcert",
    "bcertificate",
    "bcertresults",
    "bech32",
    "bitcoiners",
    "blms",
    "blogposts",
    "bytea",
    "corestore",
    "cuboplus",
    "dont",
    "esplora",
    "fontkit",
    "getenv",
    "Giacomo",
    "healthcheck",
    "inperson",
    "lnurl",
    "Nakamoto",
    "nostr",
    "openpgp",
    "opentimestamps",
    "oups",
    "paynym",
    "planb",
    "postbuild",
    "psql",
    "quickwit",
    "resave",
    "Rogzy",
    "sandclock",
    "satoshi",
    "sats",
    "secp256k1",
    "secu",
    "sendgrid",
    "superadmin",
    "tanstack",
    "traefik",
    "trpc",
    "tsbuildinfo",
    "txid",
    "typesense",
    "uuidv",
    "webp",
    "zoomable"
  ]
}
