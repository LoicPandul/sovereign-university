{
  "name": "@blms/service-user",
  "version": "0.0.1",
  "type": "module",
  "exports": {
    "development": "./src/index.ts",
    "import": "./dist/src/index.js",
    "types": "./src/index.ts"
  },
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "postbuild": "cp -r src/lib/pdf dist/src/lib/",
    "lint": "eslint --color './**/*.ts'",
    "type-check": "tsc --build",
    "check-types": "tsc --noEmit",
    "build-types": "tsc --emitDeclarationOnly -p tsconfig.json",
    "trace-performance": "npx tsc --extendedDiagnostics --noEmit -p tsconfig.json | head -n 12 > performance.log",
    "clear": "rm -rf ./dist && rm -f *.tsbuildinfo"
  },
  "dependencies": {
    "@blms/database": "workspace:*",
    "@blms/opentimestamps": "workspace:*",
    "@blms/s3": "workspace:*",
    "@pdf-lib/fontkit": "^1.1.1",
    "@trpc/server": "11.0.0-rc.433",
    "@types/qr-image": "^3.2.9",
    "argon2": "^0.41.1",
    "bip39": "^3.1.0",
    "pdf-img-convert": "^2.0.0",
    "pdf-lib": "^1.17.1",
    "qr-image": "^3.2.0",
    "stripe": "^17.5.0",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@blms/types": "workspace:*",
    "@types/uuid": "^10.0.0"
  }
}
