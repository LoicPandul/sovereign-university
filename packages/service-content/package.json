    "uuid": "^11.0.5",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@blms/types": "workspace:*",
    "@types/js-yaml": "^4.0.9",
    "@types/uuid": "^10.0.0",
    "typesense": "^1.8.2"
  }
