{
  "name": "@blms/service-content",
  "version": "0.0.1",
  "type": "module",
  "exports": {
    "development": "./src/index.ts",
    "import": "./dist/src/index.js",
    "types": "./src/index.ts"
  },
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "lint": "biome check .",
    "type-check": "tsc --build",
